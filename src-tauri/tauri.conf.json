{
  "build": {
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "gather",
    "version": "0.0.4"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "studio.darksoil.gather",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "gather.localhost",
          "windows": ["gather"],
          "plugins": ["holochain"]
        }
      ]
    },
    "windows": []
  },
  "plugins": {
    "cli": {
      "args": [
        {
          "short": "p",
          "name": "profile",
          "takesValue": true,
          "multiple": false
        }
      ]
    }
  }
}
